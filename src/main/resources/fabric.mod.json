{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Deep Pockets",
  "description": "Miner's Packs hold items you get from minings, ores and gems. The pack can only hold a few different items, but an unlimited amount of each.",
  "authors": [
    "RebelKeithy"
  ],
  "contact": {
    "sources": "https://github.com/RebelKeithy/Deep-Pockets/tree/fabric-1.17.1"
  },

  "license": "MIT",
  "icon": "assets/deep-pockets/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.rebelkeithy.deeppockets.DeepPockets"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
